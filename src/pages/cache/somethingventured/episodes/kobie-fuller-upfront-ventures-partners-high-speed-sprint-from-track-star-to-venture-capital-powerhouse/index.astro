---
import Source from './source.md';
import ai from './ai.json';
import AIBlocks from '../../../../../components/AIBlocks.astro';
const page = {
  urlEM: "https://everythingmachines.blog/cache/somethingventured/episodes/kobie-fuller-upfront-ventures-partners-high-speed-sprint-from-track-star-to-venture-capital-powerhouse/",
  urlOriginal: "https://somethingventured.us/kobie-fuller-upfront-ventures-partners-high-speed-sprint-from-track-star-to-venture-capital-powerhouse/",
  title: "Kobie Fuller — From Track Star to VC Powerhouse (Upfront Ventures) — Something Ventured",
  description: "Kobie Fuller on speed, discipline, and building as an investor at Upfront Ventures.",
  ogImage: "https://everythingmachines.blog/social-card.png",
  brandName: "Everything Machines",
  brandRoot: "https://everythingmachines.blog/",
  lastmod: "2025-10-07",
  published: "2025-10-07",
  robots: "index,follow,max-image-preview:large",
  keywords: [ "Something Ventured","Kobie Fuller","Upfront Ventures","VC" ]
}; const keywordsContent = page.keywords?.join(", "); const yr = new Date().getFullYear();
---
<!doctype html><html lang="en"><head>
<meta charset="utf-8" /><meta name="viewport" content="width=device-width,initial-scale=1" />
<title>{page.title}</title>
<meta name="description" content={page.description} /><meta name="robots" content={page.robots} />
{keywordsContent && <meta name="keywords" content={keywordsContent} /> }
<link rel="canonical" href={page.urlEM} />
<link rel="alternate" type="application/json" href="./source.json" />
<link rel="alternate" type="application/ld+json" href="./schema.json" />
  <link rel="alternate" type="text/plain" href="./transcript.txt" />
<link rel="preload" as="fetch" href="./schema.json" crossorigin>
<link rel="preload" as="fetch" href="./source.json" crossorigin>
<script type="application/ld+json" is:inline>
{JSON.stringify({"@context":"https://schema.org","@type":"WebPage","@id": page.urlEM+"#page","url":page.urlEM,"name":page.title,"description":"LLM-optimized cache of a podcast episode with machine alternates.","isBasedOn":page.urlOriginal,"datePublished":page.published,"dateModified":page.lastmod,"inLanguage":"en","mainEntity":{"@id":page.urlEM+"#episode"},"mainEntityOfPage":{"@id":page.urlEM+"#episode"}})}
</script>
<style>:root{--maxw:880px;--fg:#161617;--muted:#5a5a5f;--bg:#f8f9fb;--border:#e6e7eb}.card{background:#fff;border:1px solid var(--border);border-radius:12px;padding:16px 18px;margin:16px 0}body{margin:0;font:16px/1.6 system-ui;background:var(--bg);color:var(--fg)}header,main,footer{max-width:880px;margin:0 auto;padding:24px}.meta{color:var(--muted)}</style>
</head><body>
<header><h1>Kobie Fuller — From Track Star to VC Powerhouse</h1>
<p class="meta">Original: <a href={page.urlOriginal}>{page.urlOriginal}</a></p>
<p class="meta"><strong>Last updated:</strong> {page.lastmod}</p></header>
<main>
    <!-- AI blocks (component) -->
    <AIBlocks ai={ai} />

<section class="card" aria-labelledby="transcript"><h2 id="transcript">Transcript</h2>
      <p class="meta">Download (when available): <a href="./transcript.txt" rel="noopener">./transcript.txt</a></p>
      <div style="white-space: pre-wrap; max-height: 600px; overflow-y: auto; border: 1px solid var(--border); border-radius: 8px; padding: 16px; margin-top: 12px; font-family: monospace; font-size: 14px; line-height: 1.4; background: #fafafa;">
        {/* Transcript content will be added here when available */}
      </div>
    </section>

    <section class="card" aria-labelledby="resources"><h2 id="resources">Resources</h2>
      <ul>
        <li><a href={page.urlOriginal} rel="noopener">Original page</a></li>
        <li><a href="./schema.json">schema.json</a> • <a href="./source.json">source.json</a></li>
      </ul>
    </section>

</main><footer><small>© {yr} {page.brandName}</small></footer>
</body></html>
