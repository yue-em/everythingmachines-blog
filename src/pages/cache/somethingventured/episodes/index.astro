---
import Source from './source.md';

const page = {
  urlEM: "https://everythingmachines.blog/cache/somethingventured/episodes/",
  urlOriginal: "https://somethingventured.us/episodes",
  title: "Something Ventured — Episodes",
  description: "Episodes index for Something Ventured podcast.",
  ogImage: "https://somethingventured.us/social-card.png",
  brandName: "Everything Machines",
  brandRoot: "https://everythingmachines.blog/",
  lastmod: "2025-10-07",
  robots: "index,follow,max-image-preview:large",
  keywords: ["Something Ventured podcast","episodes index","venture capital","startups"]
};
const yr = new Date().getFullYear();
---

<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8"><meta name="viewport" content="width=device-width,initial-scale=1">
  <title>{page.title}</title>
  <meta name="description" content={page.description}>
  <link rel="canonical" href={page.urlEM}>
  <link rel="alternate" type="text/markdown" href="./source.md">
  <link rel="alternate" type="application/json" href="./source.json">
  <link rel="alternate" type="application/ld+json" href="./schema.json">

  <script type="application/ld+json" is:inline>
  {JSON.stringify({
    "@context":"https://schema.org",
    "@type":"WebPage",
    "name": page.title,
    "url": page.urlEM,
    "description":"Episodes index for a podcast series with machine alternates.",
    "isBasedOn": page.urlOriginal,
    "dateModified": page.lastmod,
    "inLanguage":"en",
    "mainEntity": { "@id":"#episodes" }
  })}
  </script>

  <script type="application/ld+json" is:inline>
  {JSON.stringify({
    "@context":"https://schema.org",
    "@type":"ItemList",
    "@id":"#episodes",
    "name":"Episodes",
    "itemListElement":[
      { "@type":"ListItem","position":1,"url":"https://everythingmachines.blog/cache/somethingventured/episodes/frank-rotman-the-midas-list-veteran-on-building-fintech-empires-at-capital-one-and-qed-investors/" },
      { "@type":"ListItem","position":2,"url":"https://everythingmachines.blog/cache/somethingventured/episodes/eric-ries-the-lean-startup-revolution-in-an-ai-driven-world/" },
      { "@type":"ListItem","position":3,"url":"https://everythingmachines.blog/cache/somethingventured/episodes/kobie-fuller-upfront-ventures-partners-high-speed-sprint-from-track-star-to-venture-capital-powerhouse/" }
    ]
  })}
  </script>

  <style>
    :root{--maxw:880px;--fg:#161617;--muted:#5a5a5f;--bg:#f8f9fb;--card:#fff;--border:#e6e7eb}
    *{box-sizing:border-box} body{margin:0;background:var(--bg);color:var(--fg);font:16px/1.6 system-ui}
    header,main,footer{max-width:var(--maxw);margin:0 auto;padding:24px}
    .meta{color:var(--muted)}
    .card{background:var(--card);border:1px solid var(--border);border-radius:12px;padding:16px 18px;margin:16px 0}
    ul{padding-left:18px}
  </style>
</head>
<body>
  <header>
    <h1>Something Ventured — Episodes</h1>
    <p class="meta">LLM-optimized index for the Something Ventured podcast. Original: <a href={page.urlOriginal}>{page.urlOriginal}</a></p>
    <p class="meta"><strong>Last updated:</strong> {page.lastmod}</p>
  </header>
  <main>
    <section class="card">
      <Source />
    </section>
  </main>
  <footer><small>© {yr} {page.brandName}</small></footer>
</body>
</html>